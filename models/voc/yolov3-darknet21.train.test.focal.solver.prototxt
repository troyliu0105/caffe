#=============== net
net: "models/voc/yolov3-darknet21.train.test.prototxt"

#=============== iter
# voc07 test has 4952 images
test_iter: 4952
#test_iter: 2476
test_interval: 100000
max_iter: 250000
iter_size: 2

#=============== optimizer
solver_mode: CPU
type: "SGD"
base_lr: 0.0001
weight_decay: 0.00001
lr_policy: "multistep"
gamma: 0.1
stepvalue: 200000
stepvalue: 225000

#=============== snapshot
snapshot: 50000
snapshot_prefix: "models/voc/yolov3-darknet21.train.test"
snapshot_after_train: true

#=============== evaluate
eval_type: "detection"
ap_version: "11point"
show_per_class_result: true

#=============== misc
display: 100
debug_info: false
test_initialization: false
average_loss: 10
random_seed: 3355411