# mask0
###############################################################################################

layer {
  name: "mask0/conv0"
  type: "Convolution"
  bottom: "block6-conv2"
  top: "mask0/conv0"
  convolution_param {
    num_output: 256
    kernel_size: 3
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
    stride: 1
    pad: 1
    bias_term: false
    group: 1
    dilation: 1
  }
}
layer {
  name: "mask0/conv0/bn"
  type: "BatchNorm"
  bottom: "mask0/conv0"
  top: "mask0/conv0"
  batch_norm_param {
    moving_average_fraction: 0.1
  }
}
layer {
  name: "mask0/conv0/scale"
  type: "Scale"
  bottom: "mask0/conv0"
  top: "mask0/conv0"
  scale_param {
    bias_term: true
    filler {
      value: 1.0
    }
    bias_filler {
      value: 0.0
    }
  }
}
layer {
  name: "mask0/conv0/relu"
  type: "ReLU"
  bottom: "mask0/conv0"
  top: "mask0/conv0"
  relu_param {
    negative_slope: 0.1
  }
}

layer {
  name: "mask0"
  type: "Convolution"
  bottom: "mask0/conv0"
  top: "mask0"
  convolution_param {
    num_output: 1
    kernel_size: 1
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
    pad: 0
    stride: 1
    bias_term: false
    engine: CUDNN
  }
}
layer {
  name: "mask0/bn"
  type: "BatchNorm"
  bottom: "mask0"
  top: "mask0"
  batch_norm_param {
    moving_average_fraction: 0.1
  }
}
layer {
  name: "mask0/scale"
  type: "Scale"
  bottom: "mask0"
  top: "mask0"
  scale_param {
    bias_term: true
    filler {
      value: 1.0
    }
    bias_filler {
      value: 0.0
    }
  }
}
layer {
  name: "mask0/sigmoid"
  type: "Sigmoid"
  bottom: "mask0"
  top: "mask0/sigmoid"
}

layer {
  name: "mask0/scaled"
  type: "Scale"
  bottom: "mask0/sigmoid"
  top: "mask0/scaled"
  param {
    lr_mult: 2.0
    decay_mult: 0.5
  }
  scale_param {
    bias_term: false
    filler {
      value: 1.0
    }
  }
}

layer {
  name: "mask0/tiled"
  type: "Tile"
  bottom: "mask0/scaled"
  top: "mask0/tiled"
  tile_param {
    axis: 1
    tiles: 512
  }
}
layer {
  name: "masked0"
  type: "Scale"
  bottom: "mask0/tiled"
  bottom: "block6-conv2"
  top: "masked0"
  scale_param {
    axis: 0
  }
}

layer {
  name: "mask0/label"
  type: "BackgroundMask"
  bottom: "mask0"
  bottom: "label"
  propagate_down: false
  propagate_down: false
  top: "mask0/label"
  background_mask_param {
    scale: 1
    sigma_scale: 1.5
  }
  include {
    phase: TRAIN
  }
}

layer {
  name: "mask0/loss"
  type: "EuclideanLoss"
  bottom: "mask0"
  bottom: "mask0/label"
  propagate_down: true
  propagate_down: false
  top: "mask0/loss"
  loss_weight: 1
  include {
    phase: TRAIN
  }
}
###############################################################################################

layer {
  name: "yolo-out-m"
